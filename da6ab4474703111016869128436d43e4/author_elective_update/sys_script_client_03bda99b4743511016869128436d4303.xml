        <condition/>
        <description>Match Password and Re-Enter Password on the User Registration form</description>
        <field>re_enter_password</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Match Password</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading) {
	   g_form.setMandatory('first_name',true);
	   g_form.setMandatory('email_id',true);
	   g_form.setMandatory('user_name',true);
	   g_form.setMandatory('password',true);
	   g_form.setMandatory('re_enter_password',true);
      return;
   }

   var pwd = g_form.getValue('password');
	var rpwd = g_form.getValue('re_enter_password');
	
	if(pwd != rpwd){
		g_form.showFieldMsg('re_enter_password','Password & Re-Enter Password should be same','error');
	}
	else if(pwd.length < 6){
		g_form.showFieldMsg('re_enter_password','Password length should be minimum 6 characters','error');
	}
   
}]]></script>
